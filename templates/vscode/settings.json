{
  "[markdown]": {
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": false
  },
  "[typescript]": {
    "editor.foldingStrategy": "indentation"
  },
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderIndicators": false,
  "diffEditor.renderSideBySide": false,
  "docker.showExplorer": false,
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "SF Mono, Vazir Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 21,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "gutter",
  "editor.selectionHighlight": false,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "typescript": "jsx",
    "typescriptreact": "jsx"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*.note": "markdown",
    ".env": "dotenv"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "githubPullRequests.telemetry.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.blame.avatars": false,
  "gitlens.blame.highlight.locations": ["gutter", "overview"],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "gitlens.views.repositories.branches.layout": "list",
  "gitlens.views.repositories.files.layout": "list",
  "html.format.wrapAttributes": "force",
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "spellright.addToSystemDictionary": true,
  "spellright.configurationScope": "user",
  "spellright.documentTypes": [
    "markdown",
    "latex"
  ],
  "spellright.language": [
    "en"
  ],
  "spellright.suggestionsInHints": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shell.osx": "/usr/local/bin/bash",
  "todo-tree.customHighlight": {
    "FIXME": {},
    "TODO": {}
  },
  "todo-tree.defaultHighlight": {
    "foreground": "green",
    "type": "none"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsnotes.defaultNotePath": "/Users/allen/Library/Mobile Documents/com~apple~CloudDocs/VSNOTES",
  "vsnotes.defaultNoteTitle": "{title}.{ext}",
  "window.title": "",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "PlasticAB",
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "vs-nomo-dark",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "welcomePage",
  "workbench.statusBar.feedback.visible": false,
  "gitlens.menus": {
    "editor": false,
    "editorGroup": {
      "blame": true,
      "compare": true
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true,
      "stashInline": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true,
      "stash": true
    }
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.enabledLanguageIds": [
    "css",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
