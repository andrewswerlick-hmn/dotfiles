{
  "[markdown]": {
    "editor.wordWrap": "on",
    "files.trimTrailingWhitespace": false
  },
  "[typescript]": {
    "editor.foldingStrategy": "indentation"
  },
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderIndicators": false,
  "diffEditor.renderSideBySide": false,
  "docker.showExplorer": false,
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "SF Mono, Vazir Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 21,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "gutter",
  "editor.selectionHighlight": false,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "typescript": "jsx",
    "typescriptreact": "jsx"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*.note": "markdown",
    ".env": "shellscript"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "githubPullRequests.telemetry.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.blame.avatars": false,
  "gitlens.blame.highlight.locations": ["gutter", "overview"],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.gitExplorer.branches.layout": "list",
  "gitlens.gitExplorer.files.layout": "auto",
  "gitlens.historyExplorer.enabled": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.enabled": false,
  "html.format.wrapAttributes": "force",
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "spellright.addToSystemDictionary": true,
  "spellright.configurationScope": "user",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shell.osx": "/usr/local/bin/bash",
  "todo-tree.customHighlight": {
    "FIXME": {},
    "TODO": {}
  },
  "todo-tree.defaultHighlight": {
    "foreground": "green",
    "type": "none"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.title": "",
  // "window.nativeTabs": true,
  // "window.titleBarStyle": "native",
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "PlasticAB",
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "vs-nomo-dark",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "welcomePage",
  "workbench.statusBar.feedback.visible": false,
  "githubPullRequests.hosts": [
    {
      "host": "https://github.com",
      "token": "system",
      "username": "oauth"
    }
  ],
  "spellright.language": ["en"],
  "spellright.documentTypes": ["markdown", "latex", "plaintext"],
  "window.zoomLevel": 0
}
